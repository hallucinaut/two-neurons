# Two Neurons Configuration Example

# API Configuration
PRIMARY_API_ENDPOINT=https://api.primary.example.com
SECONDARY_API_ENDPOINT=https://api.secondary.example.com

# API Keys
PRIMARY_API_KEY=your_primary_api_key
SECONDARY_API_KEY=your_secondary_api_key

# Timeout Settings
TIMEOUT=300

# Default Settings
DEFAULT_NEURON=primary
AUTO_CHAIN=true

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/two-neurons.log
